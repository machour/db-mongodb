{
    "name": "yiisoft/db-mongodb",
    "description": "Yii Framework MongoDB extension",
    "keywords": ["yii", "mongo", "mongodb", "active-record", "gridfs"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-mongodb/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-mongodb"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii-core": "^3.0@dev",
        "ext-mongodb": ">=1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "dev-master",
        "yiisoft/log": "dev-master",
        "yiisoft/cache": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii\\mongodb\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\mongodb\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
